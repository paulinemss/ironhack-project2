{{!-- MAIN NEWS PAGE --}}

{{> mainNav active='news' }}

<div class="wrapper">
  <h2>News</h2>
  {{#each data}}
  <div class="card-lg">
    <div class="card-middle">
      <h3>{{title}}</h3>
      <h4>Published: {{pubDate}}</h4>
      <p>{{contentSnippet}}</p>
      <a href="{{link}}" target="_blank">Read more</a>
    </div>
  </div>
  {{/each}}
</div>